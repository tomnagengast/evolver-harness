{
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(test:*)",
      "Bash(git init:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git config:*)",
      "Bash(tree:*)",
      "Bash(chmod:*)",
      "Bash(for:*)",
      "Bash(do if [ -f \"$file\" ])",
      "Bash(then echo \"Missing newline: $file\")",
      "Bash(else echo \"OK: $file\")",
      "Bash(fi)",
      "Bash(done)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "bypassPermissions"
  }
}
